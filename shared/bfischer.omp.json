{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#344675",
          "foreground": "#ffffff",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": " {{ if .SSHSession }}\uf817 {{ end }}{{ .UserName }}@{{ .HostName }} ",
          "trailing_diamond": "\ue0c6",
          "type": "session"
        },
        {
          "background": "#e14eca",
          "foreground": "#ffffff",
          "leading_diamond": "\ue0c7",
          "properties": {
            "style": "folder"
          },
          "style": "diamond",
          "template": " \ue5ff {{ .Path }} ",
          "trailing_diamond": "\ue0c6",
          "type": "path"
        },
        {
          "background": "#1d8cf8",
          "foreground": "#ffffff",
          "leading_diamond": "\ue0c7",
          "properties": {
            "branch_icon": "\ue0a0",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "diamond",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }} ",
          "trailing_diamond": "\ue0c6",
          "type": "git"
        },
        {
          "background": "#00f2c3",
          "foreground": "#ffffff",
          "leading_diamond": "\ue0c7",
          "properties": {
            "paused_icon": "\uf8e3 ",
            "playing_icon": "\ue602 ",
            "stopped_icon": "\uf04d ",
            "track_separator": " - "
          },
          "style": "diamond",
          "template": " \uf9c6 {{ .Icon }}{{ if ne .Status \"stopped\" }}{{ .Artist }} - {{ .Track }}{{ end }} ",
          "trailing_diamond": "\ue0c6",
          "type": "spotify"
        },
        {
          "background": "#ff8d72",
          "foreground": "#ffffff",
          "leading_diamond": "\ue0c7",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_version": true,
            "fetch_virtual_env": true
          },
          "style": "diamond",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "trailing_diamond": "\ue0c6",
          "type": "python"
        },
        {
          "background": "#1d8cf8",
          "foreground": "#ffffff",
          "leading_diamond": "\ue0c7",
          "powerline_symbol": "\ue0b0",
          "style": "diamond",
          "template": " {{.WorkspaceName}} ",
          "trailing_diamond": "\ue0c6",
          "type": "terraform"
        },
        {
          "background": "#fd5d93",
          "background_templates": [
            "{{ if gt .Code 0 }#d14271{{ end }}"
          ],
          "foreground": "#ffffff",
          "leading_diamond": "\ue0c7",
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "template": " \uf31b ",
          "trailing_diamond": "\ue0b4",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "palette": {
    "danger": "#fd5d93",
    "default": "#344675",
    "gray-100": "#f6f9fc",
    "gray-200": "#e9ecef",
    "gray-300": "#e3e3e3",
    "gray-400": "#ced4da",
    "gray-500": "#adb5bd",
    "gray-600": "#6c757d",
    "gray-700": "#525f7f",
    "gray-800": "#32325d",
    "gray-900": "#212529",
    "green": "#3e9022",
    "info": "#1d8cf8",
    "primary": "#e14eca",
    "red": "#de076f",
    "success": "#00f2c3",
    "teal": "#07585c",
    "warning": "#ff8d72",
    "white": "#ffffff"
  },
  "terminal_background": "#525f7f",
  "version": 2
}
